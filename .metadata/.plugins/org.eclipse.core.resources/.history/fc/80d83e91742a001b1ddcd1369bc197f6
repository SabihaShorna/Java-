import java.util.Scanner;
public class Jj {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		int j=0,c=0;
		
		int a=scan.nextInt();
		int b=scan.nextInt();
		int x=scan.nextInt();
		int arr[]= new int[c];
		for(int i=a ;i<=b;i++) {
			if(i%x==0) {
				c++;
				 for (j = 1; j <= c; j++) {
				        arr[j]=i;
				            
				        }
			}
			else 
				c=0;
		}
		
		if(c==0) {
			System.out.printf("There are no integer between %d and %d, inclusive,that is divisible by %x ", a,b,x);
		}
		else
		System.out.println(c);
		System.out.printf("There are %d integers between %d and %d,inclusive.that are divisible by %x :%d" ,c,a,b,x,arr[c]);
		

}
}
